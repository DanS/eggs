<p>
  <b>Name:</b>
  <%=h @farm.name %>
</p>


<%= link_to 'Edit', edit_farm_path(@farm) %> |
<%= link_to 'Back', farms_path %>

<h2>Pickups:</h2>
<% @farm.pickups.each do |p|%>
    <p>
      <%= link_to "#{p.name} - #{p.date}", pickup_path(p)%>
    </p>
<% end %>

<h2>Products:</h2>
<% @farm.products.each do |p|%>
    <p>
      <%= link_to "#{p.name}", product_path(p)%>
    </p>
<% end %>